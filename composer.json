    "keywords": [
        "nginx",
        "virtual host",
        "vhost",
        "apache",
        "config file",
        "manager",
        "php"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tobeorla/Nginx-Config-Processor"
        }
    ],
    "require": {
	"php": ">=5.5.0",
	"tobeorla/nginx-config-processor": "dev-patch-1",
        "tetreum/perfect-print": "dev-master",
        "tetreum/apache-vhost-processor": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*"
    },
  "autoload": {
    "psr-4": {"VHostManager\\": "src"}
  }
